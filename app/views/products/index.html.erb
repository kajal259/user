<h1>Products index</h1>


 <table border = 1>
    <thead>
        <tr>
            <th>Title</th>
            <th>Content</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>   
    <% @products.each do |product| %>
        <tr>
            <td><%= product.name%></td>
            <td><%= link_to "show",product_path(product.id)  %></td>
        </tr>
   <% end %>
  </tbody>          
</table>



